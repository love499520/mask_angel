#!name=Mask
#!desc=love
#!system=ios

[General]
# 开启 IPV6
ipv6 = true
# 隐藏 VPN
hide-vpn-icon = true

[Script]
sub_info = type=http-request,pattern=http://sub\.info,requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/TributePaulWalker/Profiles/main/JavaScript/Sub_info_lite.js,script-update-interval=0
彩云天气 = type=http-request,pattern=https:\/\/((weather-data\.apple)|(api.weather))\.com,requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/Peng-YM/QuanX/master/Tasks/caiyun.js,script-update-interval=0
彩云天气 = type=cron,cronexp="0 30 7,21 * * ?",wake-system=1,script-path=https://raw.githubusercontent.com/Peng-YM/QuanX/master/Tasks/caiyun.js,script-update-interval=0
Dler Cloud = type=cron,cronexp=0 0 * * * ?,wake-system=1,script-path=https://raw.githubusercontent.com/evilbutcher/Quantumult_X/master/check_in/glados/checkin_env.js,script-update-interval=0
Dler Cloud = type=cron,cronexp="0 31 7,21 * * ?",wake-system=1,script-path=https://raw.githubusercontent.com/Peng-YM/QuanX/master/Tasks/flow.js,script-update-interval=0
Dler Cloud Cookie = type=http-request,pattern=https:\/\/dlercloud\.com,script-path=https://raw.githubusercontent.com/evilbutcher/Quantumult_X/master/check_in/glados/checkincookie_env.js
Dler Cloud Cookie版 = type=cron,cronexp=0 1 * * * ?,wake-system=1,timeout=20,script-path=https://raw.githubusercontent.com/evilbutcher/Quantumult_X/master/check_in/glados/checkincookie_env.js,script-update-interval=0
疫情日报 = type=cron,cronexp="0 32 7,21 * * ?",wake-system=1,script-path=https://raw.githubusercontent.com/Peng-YM/QuanX/master/Tasks/nCov.js,script-update-interval=0
今日汇率 = type=cron,cronexp="0 33 7,21 * * ?",wake-system=1,script-path=https://raw.githubusercontent.com/Peng-YM/QuanX/master/Tasks/exchange.js,script-update-interval=0
# https://raw.githubusercontent.com/NobyDa/Script/master/Surge/Module/TestFlightDownload.sgmodule
TF下载修正 = type=http-request,pattern=^https?:\/\/testflight\.apple\.com\/v\d\/accounts\/.+?\/install$,requires-body=1,max-size=0,script-path=https://gist.githubusercontent.com/NobyDa/9be418b93afc5e9c8a8f4d28ae403cf2/raw/TF_Download.js,script-update-interval=0
# https://subweb.oss-cn-hongkong.aliyuncs.com/Module/embyUnlocked.sgmodule
Emby 解锁 = type=http-response,pattern=^https?:\/\/mb3admin.com\/admin\/service\/registration\/validateDevice,requires-body=1,max-size=0,script-path=https://subweb.oss-cn-hongkong.aliyuncs.com/Script/embyPremiere.js,script-update-interval=0
# 微信内链跳转
weixin110.js = type=http-response,pattern=^https?:\/\/weixin110\.qq\.com\/cgi-bin\/mmspamsupport-bin\/newredirectconfirmcgi,requires-body=1,max-size=-1,script-path=https://raw.githubusercontent.com/HotKids/Rules/master/Script/weixin110.js,script-update-interval=0

[MITM]
hostname = %APPEND% mp.weixin.qq.com, weixin110.qq.com, %APPEND% testflight.apple.com, %APPEND% mb3admin.com, dlercloud.com, weather-data.apple.com, api.weather.com, *.music.163.com, music.163.com
